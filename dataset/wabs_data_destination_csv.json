{
	"name": "wabs_data_destination_csv",
	"properties": {
		"linkedServiceName": {
			"referenceName": "lksazureblobstorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"container": {
				"type": "string"
			},
			"directory": {
				"type": "string"
			},
			"filename": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": "@dataset().filename",
				"folderPath": "@{dataset().directory}",
				"container": "@{dataset().container}"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"quoteChar": "\""
		},
		"schema": []
	}
}