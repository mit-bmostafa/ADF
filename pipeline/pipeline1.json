{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "22_3_hive_to_sqlserver",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.dw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "22_3_1_copy_core_tables",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "HiveSource",
									"query": {
										"value": "@concat('select * from reg20_master_data_core.', item().destincation.table)",
										"type": "Expression"
									}
								},
								"sink": {
									"type": "AzureSqlSink",
									"preCopyScript": {
										"value": "@{concat('truncate table reg20_master_data_core.', item().source.table)}",
										"type": "Expression"
									},
									"disableMetricsCollection": false
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "ds_hdi_hive",
									"type": "DatasetReference"
								}
							],
							"outputs": [
								{
									"referenceName": "sqlserver_reg20_master_data_core",
									"type": "DatasetReference",
									"parameters": {
										"cw_table": {
											"value": "@item().destincation.table",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"dw_items": {
				"type": "string",
				"defaultValue": [
					{
						"source": {
							"filename": "lob/lob.csv"
						},
						"destination": {
							"table": "lob"
						}
					},
					{
						"source": {
							"filename": "sub_lob/sub_lob.csv"
						},
						"destination": {
							"table": "sub_lob"
						}
					},
					{
						"source": {
							"filename": "skill/skill.csv"
						},
						"destination": {
							"table": "skill"
						}
					},
					{
						"source": {
							"filename": "lt_skill_activity/lt_skill_activity.csv"
						},
						"destination": {
							"table": "lt_skill_activity"
						}
					},
					{
						"source": {
							"filename": "lt_activity_sublob/lt_activity_sublob.csv"
						},
						"destination": {
							"table": "lt_activity_sublob"
						}
					},
					{
						"source": {
							"filename": "activity/activity.csv"
						},
						"destination": {
							"table": "activity"
						}
					},
					{
						"source": {
							"filename": "system/system.csv"
						},
						"destination": {
							"table": "system"
						}
					},
					{
						"source": {
							"filename": "company/company.csv"
						},
						"destination": {
							"table": "company"
						}
					},
					{
						"source": {
							"filename": "login_employee/login_employee.csv"
						},
						"destination": {
							"table": "login_employee"
						}
					},
					{
						"source": {
							"filename": "employee/employee.csv"
						},
						"destination": {
							"table": "employee"
						}
					}
				]
			}
		},
		"annotations": []
	}
}